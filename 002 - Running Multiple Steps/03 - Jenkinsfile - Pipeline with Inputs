pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
    stages {
        stage('build') {
	input{
	message 'Give name and age'
		parameters{
		string (name: 'NAME')
		string (age: 'AGE')
		}
	}
            steps {
                echo "$NAME"
                echo "$AGE"
            }
        }
    }
}
