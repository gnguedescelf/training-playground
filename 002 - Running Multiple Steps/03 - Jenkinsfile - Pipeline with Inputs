pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
    stages {
        stage('build') {
	input{
	message 'Give name'
		parameters{
			string (name: 'NAME')
			}
	input{
	message 'give me age'
		parameters{
			string (age: 'AGE')
				}
		}
	}
            steps {
                echo "$NAME"
                echo "$AGE"
            }
        }
    }
}
