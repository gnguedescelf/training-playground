pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
    stages {
        stage('ask for name') {
	input{
	message 'Give name'
		parameters{
			string (name: 'NAME')
			}
		}
	}
	stage('ask for age'){
	input{
	message 'give me age'
		parameters{
			string (age: 'AGE')
				}
		}
	}
          steps {
              echo "$NAME"
              echo "$AGE"
           }
      }
   
}
